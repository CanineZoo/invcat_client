{
  "schemaVersion": 1,
  "id": "invcat",
  "version": "${version}",
  "name": "InvCat-Client",
  "description": "awa",
  "authors": [],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/invcat/icon.png",
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": [
      "org.invcat.invcat.client.InvcatDataGenerator"
    ],
    "client": [
      "org.invcat.invcat.client.InvcatClient"
    ],
    "main": [
      "org.invcat.invcat.Invcat"
    ]
  },
  "mixins": [
    "invcat.mixins.json",
    {
      "config": "invcat.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
